{
  /* General Options*/
  "bIsDebug": false, /* Only enable for debugging*/
  "bOverRideModLibSettings": false, /* If enabled will force loading of config file even if modlib is enabled*/
  "modVersionOfConfig": "0.0.6", /* Currently Not Used DO NOT MODIFY*/
  "bUseFoodTypeModifiers": false, /* Enables modifying all food items by multipliers  */
  "bUseFoodMoralModifiers": false, /* Enables modifying the values for food based on moral values  */
  "bUseAnimalModifiers": false, /* Enables Animal trade good Multipliers  */
  "bUseTradeGoodsModifiers": false, /* Enables Goods trade good Multipliers  */

  /* Animal Options*/
  "weightAnimalMultiplier": "1.0", /* Multiply Animal Weight by the multiplier supplied */
  "valueAnimalMultiplier": "1.0", /* Multiply Animal Value by the multiplier supplied */


  /* General Trade Goods Options*/
  "weightGoodssMultiplier": "1.0", /* Multiply Trade goods Weight by the multiplier supplied */
  "valueGoodsMultiplier": "1.0", /* Multiply Trade goods Value by the multiplier supplied */


  /* Food by Type Options */
  "weightFoodMultiplier": "1.0", /* Multiply Food Weight by the multiplier supplied */
  "valueFoodMultiplier": "1.0", /* Multiply Food Value by the multiplier supplied */

  /* Food by Moral value Options */
  "weightFoodByMoral0Multiplier": "0.5", /* Multiply Food Weight for Moral 0 Bouns eg grain  */
  "weightFoodByMoral1Multiplier": "1.0", /* Multiply Food Weight for Moral 1 Bouns  */
  "weightFoodByMoral2Multiplier": "1.5", /* Multiply Food Weight for Moral 2 Bouns */
  "weightFoodByMoral3Multiplier": "2.0", /* Multiply Food Weight for Moral 3 Bouns */

  "valueFoodByMoral0Multiplier": "0.5", /* Multiply Food value for Moral 0 Bouns eg grain */
  "valueFoodByMoral1Multiplier": "1.0", /* Multiply Food value for Moral 1 Bouns */
  "valueFoodByMoral2Multiplier": "1.5", /* Multiply Food value for Moral 2 Bouns */
